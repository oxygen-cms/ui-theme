{
    "name": "oxygen/ui",
    "description": "Oxygen User Interface",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Ui\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen User Interface",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "images": [
            "https://raw.githubusercontent.com/oxygen-cms/ui/master/docs/login-welcome.jpg",
            "https://raw.githubusercontent.com/oxygen-cms/ui/master/docs/login-form.jpg",
            "https://raw.githubusercontent.com/oxygen-cms/ui/master/docs/pages.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui/master/docs/marketplace.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui/master/docs/pages-mobile.png"
        ],
        "providers": [
            {
                "name": "User Interface",
                "class": "Oxygen\\Ui\\UiServiceProvider",
                "description": "Injects CSS and JavaScript into the page layout."
            }
        ]
    }
}
