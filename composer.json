{
    "name": "oxygen/ui-theme",
    "description": "Oxygen User Interface",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "oxygen/preferences": "~0.3.0",
        "oxygen/auth": "~0.3.0",
        "illuminate/support": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\UiTheme\\": "src/"
        }
    },
    "extra": {
        "title": "Oxygen - UI Theme",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "images": [
            "https://raw.githubusercontent.com/oxygen-cms/ui-theme/master/docs/login-welcome.jpg",
            "https://raw.githubusercontent.com/oxygen-cms/ui-theme/master/docs/login-form.jpg",
            "https://raw.githubusercontent.com/oxygen-cms/ui-theme/master/docs/pages.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui-theme/master/docs/marketplace.png",
            "https://raw.githubusercontent.com/oxygen-cms/ui-theme/master/docs/pages-mobile.png"
        ],
        "providers": [
            {
                "name": "User Interface",
                "class": "Oxygen\\UiTheme\\UiServiceProvider",
                "description": "Injects CSS and JavaScript into the page layout."
            }
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18"
    }
}
